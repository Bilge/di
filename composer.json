{
    "name": "joomla/di",
    "type": "joomla-package",
    "description": "Joomla DI Package",
    "keywords": ["joomla", "framework", "dependency injection", "di", "ioc", "container"],
    "homepage": "https://github.com/joomla-framework/di",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10|>=7.0",
        "container-interop/container-interop": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\DI\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
